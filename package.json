    "standard": "16.0.3",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@types/node": "12.0.2",
    "collapsed-assert": "1.0.3",
    "git-hooks-plus": "1.0.1",
    "node-fetch": "2.6.7",
    "npm-bin-deps": "1.10.1",
    "tapzero": "0.6.1"
  },
  "standard": {
    "ignore": [
      "workers/go-worker-txt.js"
    ]
  },
  "author": "Raynos <raynos2@gmail.com>",
